<h1>Listing Entries</h1>

<table>
  <tr>
    <th>Date</th>
    <th>Description</th>
    <th></th>
  </tr>

  <% @entries.each do |entry| %>
    <tr>
      <td><%= entry.date %></td>
      <td><%= entry.description %></td>
      <td><%= link_to 'Show', entry_path(entry) %></td>
    </tr>
  <% end %>
</table>
